<%= month_calendar do |date| %>
    <% if DateOf.find_by(date_of: date) %>
        <%= link_to date.day, user_date_of_path(user_id: session[:user_id], id: DateOf.find_by(date_of: date).id) %>
    <% else %>
        <%= link_to date.day, new_user_date_of_path(user_id: session[:user_id], date_of: date.to_date) %>
    <% end %>
<% end %>
<%= link_to "Back", user_path(@user) %>